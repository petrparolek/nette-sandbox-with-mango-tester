{
	"name": "nette/sandbox",
	"description": "The sandbox is a pre-packaged Nette Framework project, basic configured structure for your application.",
	"homepage": "https://nette.org",
	"type": "project",
	"license": ["BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
	"authors": [
		{
			"name": "David Grudl",
			"homepage": "https://davidgrudl.com"
		},
		{
			"name": "Nette Community",
			"homepage": "https://nette.org/contributors"
		}
	],
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/petrparolek/contributte-di"
		},
		{
			"type": "git",
			"url": "https://github.com/petrparolek/mango-presenter-tester"
		},
		{
			"type": "git",
			"url": "https://github.com/petrparolek/nextras-migrations"
		}
	],
	"require": {
		"php": ">=8.0",
		"nette/application": "~3.1.0",
		"nette/bootstrap": "~3.2.0",
		"nette/caching": "~3.2.0",
		"nette/database": "~4.0.0",
		"nette/di": "~3.1.0",
		"nette/finder": "^3.0",
		"nette/forms": "~3.1.0",
		"nette/http": "~3.2.0",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/safe-stream": "^3.0",
		"nette/security": "~4.0.0",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "~2.9.0",
		"dg/adminer-custom": "^1.8",
		"contributte/console": "0.10.x-dev",
		"contributte/di": "dev-nette-4 as 0.5.4",
		"nextras/migrations": "dev-nette-4"
	},
	"require-dev": {
		"nette/tester": "~2.4.0",
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"phpstan/phpstan-doctrine": "^1.0",
		"phpstan/phpstan-mockery": "^1.0",
		"phpstan/phpstan-nette": "^1.0",
		"phpstan/phpstan-strict-rules": "^1.0",
		"contributte/qa": "^0.3",
		"mockery/mockery": "^1.0",
		"webnazakazku/mango-tester-http-mocks": "^0.4",
		"webnazakazku/mango-presenter-tester": "dev-nette-4",
		"webnazakazku/mango-tester-database-creator": "^0.4",
		"webnazakazku/mango-tester-infrastructure": "^0.6",
		"dg/bypass-finals": "^1.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"preferred-install": "source",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
